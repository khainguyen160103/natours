{
  "name": "natours",
  "version": "1.0.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "description": "learning node express moongose",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": " set NODE_ENV=development&&nodemon server.js",
    "start:prod": "set NODE_ENV=production&&nodemon server.js",
    "debug": "ndb server.js set NODE_ENV=production",
    "build": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled",
    "watch:js": "parcel watch  --no-hmr ./public/js/index.js --dist-dir ./public/js/bundled "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.35.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.1",
    "helmet": "^7.0.0",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "mongoose": "^7.4.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "ndb": "^1.1.5",
    "nodemailer": "^6.9.5",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.14.1",
